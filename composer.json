{
    "name": "iro88/phpunit-coverage-check",
    "description": "Tool to check phpunit code coverage",
    "keywords": [
        "phpunit",
        "code coverage",
        "ci"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ireneusz Derbich",
            "email": "ireneusz.derbich@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "symfony/console": "^6.0||^7.1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4.2",
        "php-mock/php-mock-mockery": "^1.4.1"
    },
    "bin": [
        "bin/phpunit-coverage-check"
    ],
    "autoload": {
        "psr-4": {
            "PhpunitCoverageCheck\\": "src/"
        }
    }
}
